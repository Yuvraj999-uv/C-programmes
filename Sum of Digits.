#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
       int x,i=1,sum = 0,y;
    scanf("%d",&x);
    if(x==0||x<=9){
        printf("%d",x);
    }
    else{
    while(x!=0){
        y=x%10;
        sum= sum+y;
        x=x/10;
          i=i+1;
        
        
        
    }
    printf("%d",sum);}

    return 0;
}
