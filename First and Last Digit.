#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x,y,z;
    scanf("%d",&x);
    if(x<0)
        x = -x;
    z=x;
    
    while(x!=0){
        y=x%10 ;           
        x=x/10;      
    }
    printf("%d",y);
    printf(" %d",z%10);
    return 0;
}
