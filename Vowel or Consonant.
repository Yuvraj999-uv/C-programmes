#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
  char ch ;
    scanf("%c",&ch);
    if(isalpha(ch)){
if(ch== 'A' || ch=='E' || ch=='I' || ch=='O' || ch=='U' ||
            ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u'){
    printf("Vowel");}
    else{
        printf("Consonant");
    }    
}
    else{
        printf("Invalid Input");
    }
    return 0;
}
