#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      int x,y,z,first,middle,last,i=0,num;
    scanf("%d",&x);
    z=x;
    last=x%10;
    if(x>=0&&x<=9){
        printf("%d",x);
    }
    else{
    while(x!=0){
        x=x/10;
        i++;
    }
   y=pow(10,i-1);
    first=z/y   ;  
    middle=(z-(first*y+last))/10;
    num=last*y+middle*10+first;
        printf("%d",num);}

    return 0;
}
