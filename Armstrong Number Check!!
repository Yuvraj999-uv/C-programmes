#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   int x,y,z,i=0,j,sum=0;
    
    scanf("%d",&x);
    z=x;
    while(x!=0){
        x=x/10;
            i++;}
    x=z;
        while(x!=0){ 
         y=x%10;
         x=x/10;
        j=pow(y,i);
        sum=sum+j;
        }
       
    if(sum==z){
        printf("Armstrong");
    }
    else{
         printf("Not Armstrong");
    }
    
    
    
   
    return 0;
}
