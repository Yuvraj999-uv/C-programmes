#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int x,i=1 ;
    scanf("%d",&x);
    
    for(i=1;i<=x;i++){
        for(int j=1;j<=x;j++){
            printf("* ");
        
        }
        
            printf("\n");
        }
        
    
    
    return 0;
}
