#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      int x,y;
    scanf("%d",&x);
    if(x>=0&&x<=9){
        printf("%d",x);
    }
    else{
    while(x!=0){
         y=x%10;
        printf("%d",y);
        x=x/10;
   //     if(x==0){
     //       printf("0");
        //
    
    }   }
    return 0;
}
