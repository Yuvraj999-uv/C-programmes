#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float r,ac,pc,a,as,ps,l,b,ar,pr;
    scanf("%f\n%f\n%f%f",&r,&a,&l,&b);
    ac=3.1416*r*r;
    pc=3.1416*2*r;
    as=a*a;
    ps=4*a;
    ar=l*b;
    pr=2*(l+b);
    printf("Circle: %.2f %.2f\nSquare: %.2f %.2f\nRectangle: %.2f %.2f",ac,pc,as,ps,ar,pr);
    
        
    
    

    return 0;
}
