#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x,i,a,max;
    scanf("%d",&x);
    scanf("%d",&a);
    max=a;
    for(i=1;i<=x;i++){
        
        scanf("%d",&a);
        if(a>max){
            max=a;
        }
        
        
    }
    printf("%d",max);
    return 0;
}
